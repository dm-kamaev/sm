{
    "formatters": {
        "details": {
            "format": "[%(date)s] %(levelname)s %(message)s",
            "strip": true
        }
    },
    "handlers": {
        "terminal": {
            "formatter": "details",
            "class": "intel/handlers/console"
        }
    },
    "loggers": {
        "app": {
            "handlers": ["terminal"]
        },
        "express": {
            "handlers": ["terminal"]
        },
        "sequelize": {
            "handlers": ["terminal"]
        }
    }
}
