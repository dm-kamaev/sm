					школы мела
					<span class="b-header__stars">
						<span class="b-header__star b-header__star_fifth">★</span>
						<span class="b-header__star b-header__star_forth">★</span>
						<span class="b-header__star b-header__star_third">★</span>
						<span class="b-header__star b-header__star_second">★</span>
						<span class="b-header__star b-header__star_first">★</span>
					</span>
				</div>
				<div class="b-header__city">
					москва
				</div>
			</div>
		</div>
		
		<div class="b-header__search">
			<div class="b-header__search-image">
				<div class="b-header__line b-header__line_left">
				</div>
			</div>
		</div>
	</div>
{/template}
