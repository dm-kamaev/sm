{namespace sm.bHeader.Template}
/**
* Header
*/
{template .base autoescape="strict" kind="html"}
    <div class="b-header">
		<div class="b-header__menu">
			<div class="b-header__menu-image">
				<div class="b-header__line b-header__line_right">
				</div>
			</div>
		</div>
		
		<div class="b-header__main">
			<div class="b-header__before">
			</div>
			<div class="b-header__wrap">
				<div class="b-header__title">
					<span class="b-header__stars">
						<span class="b-header__star b-header__star_first">★</span>
						<span class="b-header__star b-header__star_second">★</span>
						<span class="b-header__star b-header__star_third">★</span>
